{
  "manifest_version": 2,
  "name": "Catalog filler",
  "version": "1.0",
  "default_locale": "ru",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/content.js"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "<all_urls>"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}